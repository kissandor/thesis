<UserControl x:Class="ExcelUploadClient.View.ComputerParts"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ExcelUploadClient.View"
             xmlns:src="clr-namespace:ExcelUploadClient.VMVM.Model"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <DataGrid x:Name="DataGridComputerParts" 
                  Margin="10,50,10,10"
                  ItemsSource="{Binding ComputerParts}">
            
        </DataGrid>
    </Grid>
</UserControl>
